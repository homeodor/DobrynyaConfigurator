{
  "actions" : {
    "build" : {
      "postActions" : [
        {
          "script" : "npm run build",
          "type" : "runScript"
        }
      ]
    },
    "run" : {
      "postActions" : [
        {
          "script" : "npm run dev",
          "type" : "runScript"
        }
      ]
    }
  },
  "buildBeforeRunning" : true,
  "extension" : {
    "identifier" : "panic.JavaScript",
    "name" : "JavaScript"
  },
  "extensionTemplate" : "nodeDebug"
}
